  "scripts": {
    "clean:deps": "npx rimraf node_modules package-lock.json yarn.lock pnpm-lock.yaml && npm cache clean --force",
    "clean:builds": "npx rimraf dist build",
    "svgo": "svgo -f plugins/theme/icons --config=./svgo.yml",
    "lint": "eslint . --fix --ext .jsx,.js,.vue,.ts,.tsx",
    "format": "prettier --write .",
    "serve:dev": "vite serve docs --host 0.0.0.0",
    "build:prod": "pnpm run lint && pnpm run clean:builds && webpack --mode production",
    "build:dev": "pnpm run clean:builds && webpack --mode development",
    "upd": "hlx up",
    "rel": "hlx rel",
    "pub": "hlx pub",
    "prepub": "pnpm run build:prod",
    "prepare": "husky install",
    "reinstall": "pnpm clean:deps && pnpm clean:builds && pnpm i"
